package songlib;

import java.util.ArrayList;

public class SongList extends ArrayList<Song> {
	
	private static final long serialVersionUID = 1;
	
	public boolean add(Song song) {
		if (this.size() > 0) {
			int index = sort(song, 0, size() - 1);
		} else {
			super.add(song);
		}
		
		return true;
	}
	
}
