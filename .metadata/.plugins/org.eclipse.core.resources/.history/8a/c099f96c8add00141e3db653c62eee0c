		public BufferedImage getPhoto();
	}
	
	private static final long serialVersionUID = 1;
	private BufferedImage keyPhoto, defaultPhoto;
	
	public PhotoButton(Integer index) {
		
	}
	
	public void resized(Dimension size) {
		
	}

}
